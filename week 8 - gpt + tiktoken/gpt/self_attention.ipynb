    "   \"source\": [\n",
    "    \"import torch\\n\",\n",
    "    \"import torch.nn as nn\\n\",\n",
    "    \"from torch.nn import functional as F\\n\",\n",
    "    \"torch.set_default_device(\\\"mps\\\")  # use gpu\\n\",\n",
    "    \"torch.manual_seed(1337)  # seed for reproducibility\"\n",
    "   ],\n",
    "   \"outputs\": [\n",
